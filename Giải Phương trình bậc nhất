#include <stdio.h>
#include <math.h>


int main() {
	float a, b, x;
	printf("Nhap a, b: ");
	scanf_s("%f%f", &a, &b);
	if (a == 0 && b != 0) {
		printf("PT vo nghiem: \n");
	}
	else if (a == 0 && b == 0) {
		printf("PT co vo so nghiem: \n");
	}
	else
	{
		x = -b / a;
		printf("PT co nghiem la: %0.2f", x);
	}
	return 0;
}
