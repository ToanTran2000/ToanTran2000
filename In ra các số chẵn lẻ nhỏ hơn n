#include <stdio.h>
#include <math.h>


int main() {
	int n;
	printf("Nhap n > 0 ");
	scanf_s("%d", &n);
	if (n <= 0) {
		printf("Nhap n > 0");
	}
	else
	{
		int i;
		printf("Cac so chan: ");
		// in ra so chan 
		for (i = 0; i < n; i+= 2) {
			printf("%d ", i);
		}
		printf("\n\n");
		printf("Cac so le: ");
		// in ra so le 
		for (i = 1; i < n; i += 2) {
			printf("%d ", i);
		}
	}
	return 0;
}
