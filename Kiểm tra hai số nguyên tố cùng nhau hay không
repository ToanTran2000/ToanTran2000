#include <stdio.h>
#include <math.h>




int main() {
	int m, n;
	printf("Nhap hai so m va n > 0: ");
	scanf_s("%d %d", &m, &n);
	if (m > 0 && n > 0) {
		while (m != n)
		{
			if (m > n) {
				m = m - n;
			}
			else
			{
				n = n - m;
			}
		}
		// uoc chung m, n
		if (m == 1) {
			printf("Hai so nguyen to cung nhau");
		}
		else
		{
			printf("Hai so khong nguyen to cung nhau");
		}
	}
	else
	{
		printf("Nhap lai m,n > 0.");
	}
	return 0;
}
